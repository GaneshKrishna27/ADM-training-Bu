
@{
    ViewBag.Title = "book";
}

<h2>book</h2>
@{
    var selectedBoquetData = ViewBag.boquet;
}
<body>
    @using (Html.BeginForm())
    {
        <div>
            <img src="@selectedBoquetData.imagepath" />
        </div>
        <div>   
            <div itemprop="name">@selectedBoquetData.boquetName</div>
            <span>
                <span class="Price">₹.</span>
                <span>@selectedBoquetData.price</span>
            </span>
        </div>
    }
</body>